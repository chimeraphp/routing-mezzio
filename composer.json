{
    "name": "chimera/routing-mezzio",
    "description": "Laminas Mezzio implementation for chimera/routing",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lu√≠s Cobucci",
            "email": "lcobucci@gmail.com"
        }
    ],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "chimera/foundation": "^0.4@dev",
        "chimera/routing": "^0.4@dev",
        "laminas/laminas-diactoros": "^2.4",
        "mezzio/mezzio": "^3.2",
        "mezzio/mezzio-fastroute": "^3.0",
        "mezzio/mezzio-helpers": "^5.3"
    },
    "require-dev": {
        "infection/infection": "^0.16",
        "lcobucci/coding-standard": "^6.0",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-deprecation-rules": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-strict-rules": "^0.12",
        "phpunit/phpunit": "^9.4"
    },
    "provide": {
        "chimera/routing-implementation": "0.4.0"
    },
    "replace": {
        "chimera/routing-expressive": "*"
    },
    "autoload": {
        "psr-4": {
            "Chimera\\Routing\\Mezzio\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chimera\\Routing\\Mezzio\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4.x-dev"
        }
    }
}
